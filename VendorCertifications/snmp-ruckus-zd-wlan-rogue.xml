<DataModel namespace="http://im.ca.com/certifications/snmp" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="SNMPCertificationFacet.xsd">
  <FacetType name="RuckusZDWLANRogueMib" descriptorClass="com.ca.im.core.datamodel.certs.CertificationFacetDescriptorImpl">
    <Documentation>Ruckus ZD WLAN Rogue</Documentation>
    <FacetOf namespace="http://im.ca.com/core" name="Item" />
      <Protocol>SNMP</Protocol>
      <MIB>AUTO-GENERATION-MIB-NAME</MIB>
      <DisplayName>Ruckus ZD WLAN Rogue</DisplayName>
    <AttributeGroup name="RuckusZDWLANRogueGroup" external="true">
        <Attribute name="Index" type="ObjectID">
          <IsKey>true</IsKey>
          <IsIndex>true</IsIndex>
          <Source>1.3.6.1.4.1.25053.1.2.2.1.1.4.1.1.1</Source>
        </Attribute>
        <Attribute name="ruckusZDWLANRogueMacAddr" type="OctetString">
          <IsKey>true</IsKey>
          <NeedsDelta>false</NeedsDelta>
          <Source>1.3.6.1.4.1.25053.1.2.2.1.1.4.1.1.1</Source>
        </Attribute>
        <Attribute name="ruckusZDWLANRogueSSID" type="OctetString">
          <IsKey>false</IsKey>
          <NeedsDelta>false</NeedsDelta>
          <Source>1.3.6.1.4.1.25053.1.2.2.1.1.4.1.1.2</Source>
        </Attribute>
        <Attribute name="ruckusZDWLANRogueChannel" type="Long">
          <IsKey>false</IsKey>
          <NeedsDelta>false</NeedsDelta>
          <Source>1.3.6.1.4.1.25053.1.2.2.1.1.4.1.1.4</Source>
        </Attribute>
        <Attribute name="ruckusZDWLANRogueSignalStrength" type="Long">
          <IsKey>false</IsKey>
          <NeedsDelta>false</NeedsDelta>
          <Source>1.3.6.1.4.1.25053.1.2.2.1.1.4.1.1.11</Source>
        </Attribute>
    </AttributeGroup>
    <Expressions>
      <ExpressionGroup destCert="{http://im.ca.com/normalizer}NormalizedWirelessInterfaceInfo" name="RuckusZDWLANRogueDS">
            <Expression destAttr="Indexes">Index</Expression>
             <Expression destAttr="Names">"Rogue " + ruckusZDWLANRogueSSID + ", MAC " + ruckusZDWLANRogueMacAddr</Expression>
             <Expression destAttr="Descriptions">"Ruckus WLAN Rogue " + ruckusZDWLANRogueSSID + ", MAC " + ruckusZDWLANRogueMacAddr</Expression>
             <Expression destAttr="ChannelNum">ruckusZDWLANRogueChannel</Expression>
             <Expression destAttr="SignalStrength">ruckusZDWLANRogueSignalStrength</Expression>
      </ExpressionGroup>
    </Expressions>
  </FacetType>
</DataModel>